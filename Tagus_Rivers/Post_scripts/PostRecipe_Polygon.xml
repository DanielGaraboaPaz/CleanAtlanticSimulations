<?xml version="1.0" encoding="UTF-8" ?>
<postProcessing>
	<time>
        <step value = "250" />
	</time>
	<EulerianMeasures>
		<measures>
			<field key = "concentrations"/>
			<filters>
				<filter key = "beaching" value= "0" comments = "0-all, 1-only non beached particles, 2-only beached (defaut=0)"/>
			</filters>
		</measures>
		<polygonDefinition>
			<file name= "./Post_scripts/MRU/MarineReportingUnits_MSFD_2018_v2.shp" comments="relative, meters, degrees"/>
			<resolution x="200" y="200" z="1"/>
		</polygonDefinition>
	</EulerianMeasures>	
    <plot>
        <time key= "" value= "all" comments="season, month, year"/>
        <measure key="sum" comments="any implicit method, mean, std, diff, cumsum"/>
        <type value="imshow" comments="contour,contourf,pcolormesh,imshow"/> 
    </plot>
</postProcessing>
